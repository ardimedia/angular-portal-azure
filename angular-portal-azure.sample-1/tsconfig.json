{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "module": "none",
    "moduleResolution": "node",
    "outFile": "build/app.js",
    "declaration": true,
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false
  },
  "exclude": [
    "azureportalng",
    "build",
    "node_modules"
  ]
}
